{
  "name": "tracked-built-ins",
  "version": "3.1.1",
  "description": "Tracked versions of JavaScript's built-in classes!",
  "keywords": [
    "ember-addon"
  ],
  "repository": "https://github.com/tracked-tools/tracked-built-ins",
  "license": "MIT",
  "author": "Chris Hewell Garrett",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "lint": "npm-run-all lint:*",
    "lint:template-lint": "ember-template-lint .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "start": "ember serve",
    "test": "npm-run-all lint:* test:*",
    "test:ember": "ember test",
    "test:ember-compatibility": "ember try:each",
    "prepublishOnly": "ember ts:precompile",
    "postpublish": "ember ts:clean"
  },
  "dependencies": {
    "ember-cli-babel": "^7.26.10",
    "ember-cli-typescript": "^5.1.0",
    "ember-tracked-storage-polyfill": "^1.0.0"
  },
  "devDependencies": {
    "@ember/optional-features": "^2.0.0",
    "@ember/test-helpers": "^2.5.0",
    "@glimmer/component": "^1.0.4",
    "@glimmer/tracking": "^1.0.4",
    "@types/ember-qunit": "~5.0.0",
    "@types/ember-resolver": "^5.0.10",
    "@types/ember__application": "~4.0.0",
    "@types/ember__debug": "~4.0.0",
    "@types/ember__test-helpers": "~2.6.0",
    "@types/qunit": "^2.11.1",
    "@types/rsvp": "^4.0.3",
    "@typescript-eslint/eslint-plugin": "^5.23.0",
    "@typescript-eslint/parser": "^5.23.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-auto-import": "^2.2.2",
    "ember-cli": "~4.3.0",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-htmlbars": "^6.0.0",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-typescript-blueprints": "^3.0.0",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.2",
    "ember-maybe-import-regenerator": "^1.0.0",
    "ember-qunit": "^5.1.5",
    "ember-resolver": "^8.0.3",
    "ember-source": "~4.4.0",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^4.7.0",
    "ember-try": "^2.0.0",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-ember": "^10.4.1",
    "eslint-plugin-node": "^11.1.0",
    "expect-type": "^0.13.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.4.1",
    "qunit": "2.19.1",
    "qunit-dom": "^2.0.0",
    "release-it": "^14.6.1",
    "release-it-lerna-changelog": "^4.0.1",
    "typescript": "^4.1.3",
    "webpack": "^5.59.1"
  },
  "engines": {
    "node": "14.* || 16.* || >= 18.*"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  },
  "prettier": {
    "singleQuote": true
  },
  "release-it": {
    "plugins": {
      "release-it-lerna-changelog": {
        "infile": "CHANGELOG.md"
      }
    },
    "git": {
      "tagName": "v${version}"
    },
    "github": {
      "release": true
    }
  },
  "volta": {
    "node": "14.19.2",
    "yarn": "1.22.10"
  }
}
